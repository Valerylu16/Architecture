.MODEL SMALL
.STACK 200H
.DATA
    array DB '1','2','3','4'
.CODE
    ;CODE SEGMENT
    MAIN PROC ;INT MAIN
        
        MOV AX, @DATA
        MOV DS, AX
        
        MOV SI, offset array
        
        MOV CX, 100H
        L1: MOV DX, [SI]
            PUSH DX
            INC SI
        LOOP L1
        
        MOV CX, 04H
        L2: POP DX
            MOV AH, 2
            INT 21H
        LOOP L2
        
        MOV AH, 4CH
        INT 21H
        
        MAIN ENDP
    END MAIN
